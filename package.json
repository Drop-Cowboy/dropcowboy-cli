{
    "name": "dropcowboy",
    "version": "1.0.2",
    "description": "DropCowboy SDK + CLI, making it easy to send ringless voicemails and SMS messages.",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/types/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "bin": {
        "dropcowboy": "dist/cli/index.js"
    },
    "scripts": {
        "build:cjs": "tsc --project tsconfig.cjs.json",
        "build:esm": "tsc --project tsconfig.esm.json",
        "build": "npm run build:cjs && npm run build:esm",
        "start": "node dist/cli/index.js",
        "prepare": "npm run build",
        "test": "jest tests/unit --runInBand",
        "test:integration": "jest tests/integration --runInBand"
    },
    "dependencies": {
        "commander": "^10.0.0",
        "dotenv": "^17.2.3",
        "node-fetch": "^2.6.7"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/node": "^20.0.0",
        "@types/node-fetch": "^2.6.13",
        "jest": "^29.7.0",
        "ts-jest": "^29.4.4",
        "typescript": "^5.0.0"
    },
    "directories": {
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Drop-Cowboy/dropcowboy-cli.git"
    },
    "keywords": [
        "ringlessvoicemail",
        "sms",
        "rvm",
        "voicebroadcast",
        "textmessaging",
        "voicedrop",
        "dropcowboy"
    ],
    "author": "drop cowboy",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Drop-Cowboy/dropcowboy-cli/issues"
    },
    "homepage": "https://github.com/Drop-Cowboy/dropcowboy-cli#readme"
}
